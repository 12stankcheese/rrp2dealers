-- Scripts:

local function TLUSIO_fake_script() -- TextButton.secondScript 
	local script = Instance.new('LocalScript', TextButton)

	local db = true
	local player = game.Players.LocalPlayer
	local delay = 3
	
	script.Parent.MouseButton1Click:Connect(function()
		if db == true and game.Workspace.Dealers:FindFirstChild("Dealer") then
			db = false
			player.Character.HumanoidRootPart.CFrame = game.Workspace.Dealers.Dealer.Head.CFrame * CFrame.new(0, 3, 0)
			for i = delay, 1, -1 do
				script.Parent.Text = i
				wait(1)
			end
			script.Parent.Text = "Dealer1"
			db = true
		elseif db == true and not game.Workspace.Dealers:FindFirstChild("Dealer") then
			script.Parent.Text = "Dealer Not Found"
			db = false
			wait(1)
			script.Parent.Text = "Dealer1"
			db = true
		end
	end)
end
coroutine.wrap(TLUSIO_fake_script)()
local function WQZSY_fake_script() -- TextButton_2.secondScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local db = true
	local player = game.Players.LocalPlayer
	
	local delay = 3
	
	script.Parent.MouseButton1Click:Connect(function()
		if db == true and game.Workspace.Dealers:FindFirstChild("Dealer2") then
			db = false
			player.Character.HumanoidRootPart.CFrame = game.Workspace.Dealers.Dealer2.Head.CFrame * CFrame.new(0, 3, 0)
			for i = delay, 1, -1 do
				script.Parent.Text = i
				wait(1)
			end
			script.Parent.Text = "Dealer2"
			db = true
		elseif db == true and not game.Workspace.Dealers:FindFirstChild("Dealer2") then
			script.Parent.Text = "Dealer Not Found"
			db = false
			wait(1)
			script.Parent.Text = "Dealer2"
			db = true
		end
	end)
end
coroutine.wrap(WQZSY_fake_script)()
